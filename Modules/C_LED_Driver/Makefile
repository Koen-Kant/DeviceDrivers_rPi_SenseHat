# Makefile for CModule

COMPONENT   = ledmodule
OBJS        = LEDModule
CMHGDEPENDS = LEDModule
CUSTOMRES   = no

include CModule

# This example has no 'h' directory, but one needs to be created to hold
# the header file generated by CMHG.
${DIRS} ::
        ${MKDIR} h

# Clean should remove the 'h' directory created above.
clean ::
        ${WIPE} h ${WFLAGS}

# Dynamic dependencies:
h.ledmoduleHdr:	cmhg.ledmoduleHdr
o.ledmoduleHdr:	cmhg.ledmoduleHdr
o.LEDModule:	c.LEDModule
o.LEDModule:	C:h.kernel
o.LEDModule:	C:h.swis
o.LEDModule:	h.ledmodulehdr
